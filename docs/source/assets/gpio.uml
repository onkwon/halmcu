@startuml
interface GPIO {
	set_mode()
	set_altfunc()
	clear_event()
	write_port()
	read_port()
	write()
	read()
}

class LL {
	reset()
	enable_port()
	disable_port()
	set_mode()
	set_altfunc()
	set_speed()
	set_debouncer()
	enable_irq()
	disable_irq()
	clear_event()
	write_port()
	read_port()
	write()
	read()
}

class HAL {
	open()
	open_output()
	close()
}

HAL ..|> GPIO
HAL --> LL
HAL ..> LL_power
HAL ..> LL_clock
HAL ..> IRQ
LL --> GPIO
@enduml
