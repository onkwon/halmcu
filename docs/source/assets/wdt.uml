@startuml
interface wdt {
	set_prescaler()
	get_prescaler()
	set_reload()
	get_reload()
	feed()
	start()
	stop()
	set_debug_stop_mode()
}

class LL {
	reset()
	get_count()
	set_reload_ms()
	set_interrupt()
	is_event_raised()
	set_clock_source()
	get_clock_source()
}

class HAL {
	enable()
	disable()
	get_clock_freqeuncy()
}

HAL ..|> wdt
HAL --> LL
HAL ..> LL_power
HAL ..> LL_clock
HAL ..> IRQ
LL --> wdt
@enduml
