@startuml
interface wdt {
	set_prescaler()
	get_prescaler()
	set_reload()
	get_reload()
	feed()
	start()
	stop()
	set_debug_stop_mode()
}

class LL_wdt {
	reset()
	get_count()
	set_interrupt()
	is_event_raised()
	set_clock_source()
	get_clock_source()
}

class HAL_wdt {
	enable()
	disable()
	set_reload_ms()
	get_clock_freqeuncy()
}

wdt <|.. LL_wdt
LL_wdt ..> LL_clock
HAL_wdt --> LL_wdt
HAL_wdt ..> LL_power
HAL_wdt ..> LL_clock
HAL_wdt ..> IRQ
@enduml
