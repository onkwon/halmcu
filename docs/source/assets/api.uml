@startuml
package Interface <<Rectangle>> {
	interface GPIO
	interface TIMER
}

package LL <<Rectangle>> {
	ABOV ..|> GPIO
	STM32 ..|> GPIO
	nRF52 ..|> GPIO
	ABOV ..|> TIMER
	STM32 ..|> TIMER
	nRF52 ..|> TIMER
}

package HAL <<Rectangle>> {
}

package AbstactDataType <<Rectangle>> {
	abstract peripheral_t
	peripheral_t <-- GPIO
	peripheral_t <-- TIMER
}

HAL --> LL
@enduml
